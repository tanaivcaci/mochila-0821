//GIT BASH
cd               cambiar directorio
cd ..            Volver atras en el directorio                         
clear            limpiar terminal
mkdir            crear carpeta
touch            crear archivo
rm               borrar archivo
rm -R            borrar archivos y despues carpeta
rmdir            borrar carpeta
mv               mover archivo o cambiar nombre del mismo
pwd              donde estamos trabajando
ls                lista de raiz
ls -a             lista de raiz detallada
ls -R              lista detallada completa


//GIT

git init                              crear repositorio local
git config user.name "   "             usuario para un repositoro especifico
git congig user.email "   "            mail para un repositorio especifico
git config global user.name "  "       usuario para repositorio global
git config global user.email "   "     mail para repositorio global
git add                                agregar archivo al repositorio
git add .                              agragar archivos al repositorio
git log                                registrar un historial de los cambios de nuestros proyectos
git checkout                           accion para volver atras
git branch -M main                     cambiar rama principal a main
git push -u origin main                subir archivos a la nube remota
git pull origin main                   bajar archivos al repositorio local
git clone +urlgithub                   crear un copia de todos los archivos en la nube remota a local
git remote add origin +urlgithub        vincula repositorio local a remoto
git branch                              enumera todas las ramas de tu repositorio local
git branch <rama>                       crear rama
git checkout <rama>                     para moverse entre ramas
git branch -d <rama>                    elimina la rama, Git evita eliminacion si no se guardo
git branch -D <rama>                    forzar eliminacionde rama por si no se guardo
git push origin <rama>                  para subir cambios a la nube remota
git merge <rama>                        fucionar cambios en la rama que uno esta
git --graph --online                     ver todas las ramas mas  grafiacamente
git reset --hard <commitnumero>          volver atras en commits sobre tus archivos